          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="EntitiesTableAdapter" GeneratorDataComponentClassName="EntitiesTableAdapter" Name="Entities" UserDataComponentName="EntitiesTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="MenuMasterConnectionString (Settings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="False" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="False" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>
                      ;WITH schemas(schemaname, remain)
                      as (
                      SELECT    left(@schmas , isnull(nullif(CHARINDEX(',',@schmas),0),len(@schmas)+1)-1)
                      as schemaname
                      ,  right(@schmas, len(@schmas) - isnull(nullif(CHARINDEX(',',@schmas),0),len(@schmas)) )
                      as remain
                      UNION  ALL
                      SELECT left(schemas.remain , isnull(nullif(CHARINDEX(',',schemas.remain),0),len(schemas.remain)+1)-1)
                      as schemaname
                      ,  right(schemas.remain, len(schemas.remain) - isnull(nullif(CHARINDEX(',',schemas.remain),0),len(schemas.remain)) )
                      as remain
                      FROM schemas Where isnull(len(schemas.remain),0) &gt; 0
                      )
                      , TableList (TABLE_CATALOG, TABLE_SCHEMA, TABLE_NAME, TABLE_TYPE, schemaname)
                      AS (
                      SELECT     TABLE_CATALOG
                      ,TABLE_SCHEMA
                      , TABLE_NAME
                      ,TABLE_TYPE
                      ,schemas.schemaname
                      FROM      INFORMATION_SCHEMA.TABLES tabs
                      INNER JOIN schemas on schemas.schemaname = tabs.TABLE_SCHEMA
                      WHERE ( isnull(@includeTable,1) = 1 AND TABLE_TYPE = 'BASE TABLE')
                      or ( isnull(@includeView,0) = 1 AND TABLE_TYPE = 'VIEW')
                      )
                      , Result
                      as (
                      Select TABLE_CATALOG
                      ,TABLE_SCHEMA
                      ,TABLE_NAME
                      ,TABLE_TYPE

                      , schemaname
                      from TableList
                      UNION
                      select pkTabs.TABLE_CATALOG
                      , pkTabs.TABLE_SCHEMA
                      ,  pkTabs.TABLE_NAME
                      ,  'BASE TABLE' TABLE_TYPE
                      , pkTabs.TABLE_SCHEMA
                      FROM [INFORMATION_SCHEMA].[REFERENTIAL_CONSTRAINTS] refs
                      inner join [INFORMATION_SCHEMA].[TABLE_CONSTRAINTS] pkTabs on refs.UNIQUE_CONSTRAINT_NAME = pkTabs.CONSTRAINT_NAME
                      inner join [INFORMATION_SCHEMA].[TABLE_CONSTRAINTS] fkTabs on refs.CONSTRAINT_NAME = fkTabs.CONSTRAINT_NAME
                      INNER JOIN TableList tl ON tl.TABLE_CATALOG = fkTabs.TABLE_CATALOG AND tl.TABLE_SCHEMA = fkTabs.TABLE_SCHEMA AND tl.TABLE_NAME = fkTabs.TABLE_NAME
                      Where ISNULL(@includeFK, 0) = 1
                      )
                      Select TABLE_CATALOG
                      , TABLE_SCHEMA
                      , TABLE_NAME
                      , 'BASE TABLE' TABLE_TYPE
                      , convert(varchar(55),newid()) AS OBJECT_ID
                      , TABLE_SCHEMA
                      FROM Result
                    </CommandText>
